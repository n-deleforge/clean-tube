{
    "manifest_version": 2,
    "name": "CleanTube",
    "version": "1.3.1",
    "description": "Make your Youtube cleaner.",
	"icons" : {
		"128" : "icon.png"
	},
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "js": ["cleanTube.js"]
        }
    ]
}